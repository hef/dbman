{
  "plugins": {
    "commit-analyzer": {
      "name": "default"
    },
    "ci-condition": {
      "name": "default"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "true"
      }
    },
    "files-updater": {
      "names": ["helm"],
      "options": {
        "helm_update_appversion": "true",
        "helm_appversion_template": "%s"
      }
    }
  }
}